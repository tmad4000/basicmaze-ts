{
  "name": "app-typescript",
  "description": "Stable version of a basic TypeScript example with SCSS.",
  "license": "MIT",
  "version": "2.4.2",
  "scripts": {
    "prod": "webpack --env.NODE_ENV=production",
    "dev": "webpack --env.NODE_ENV=development",
    "start": "webpack-dev-server --env.NODE_ENV=development",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "jest",
    "test:e2e": "start-server-and-test start http-get://localhost:4000 \"cypress run\"",
    "test:e2e:dev": "start-server-and-test start http-get://localhost:4000 \"cypress open\"",
    "lint": "npm run lint:scripts && npm run lint:styles",
    "lint:scripts": "eslint --fix . --ext .ts src",
    "lint:styles": "stylelint \"./src/**/*.scss\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint"
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@types/lodash": "^4.14.149",
    "bootstrap": "^4.4.1",
    "jquery": "3.4.1",
    "lodash": "^4.17.15",
    "normalize-scss": "7.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@cypress/webpack-preprocessor": "^4.1.1",
    "@types/chai": "^4.2.7",
    "@types/jest": "^25.1.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "autoprefixer": "^9.7.4",
    "babel-jest": "^25.1.0",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.4.2",
    "cypress": "^3.8.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "file-loader": "^5.0.2",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "jest-transform-stub": "^2.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^3.0.0",
